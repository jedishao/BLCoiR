&lt;p&gt;This is because of rome PropertiesLoader&lt;/p&gt;

&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
    &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt; PropertiesLoader getPropertiesLoader() {
        &lt;span class=&quot;code-keyword&quot;&gt;synchronized&lt;/span&gt;(PropertiesLoader.class) {
            PropertiesLoader loader = (PropertiesLoader)
                clMap.get(&lt;span class=&quot;code-object&quot;&gt;Thread&lt;/span&gt;.currentThread().getContextClassLoader());
            &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (loader == &lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;) {
                &lt;span class=&quot;code-keyword&quot;&gt;try&lt;/span&gt; {
                    loader = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; PropertiesLoader(MASTER_PLUGIN_FILE, EXTRA_PLUGIN_FILE);
                    clMap.put(&lt;span class=&quot;code-object&quot;&gt;Thread&lt;/span&gt;.currentThread().getContextClassLoader(), loader);
                }
                &lt;span class=&quot;code-keyword&quot;&gt;catch&lt;/span&gt; (IOException ex) {
                    &lt;span class=&quot;code-keyword&quot;&gt;throw&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; RuntimeException(ex);
                }
            }
            &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; loader;
        }
    }
&lt;/pre&gt;
