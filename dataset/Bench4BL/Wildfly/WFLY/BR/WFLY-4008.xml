<item>
            <title>[WFLY-4008] MBean deployment causes  javax.naming.NameNotFoundException: java:comp/BeanManager</title>
                <link>https://issues.jboss.org/browse/WFLY-4008</link>
                <project id="12313721" key="WFLY">WildFly</project>
                    <description>&lt;p&gt;If you create an MBean in your EJB and deploy the EJB.&lt;/p&gt;


&lt;p&gt;&amp;lt;pre&amp;gt;&lt;br/&gt;
@SuppressWarnings(&quot;serial&quot;)&lt;br/&gt;
public class Monitor implements MonitorMXBean, Serializable {&lt;/p&gt;

&lt;p&gt;	private static final Logger log = LoggerFactory.getLogger(Monitor.class);&lt;/p&gt;

&lt;p&gt;	private Boolean enabled = Boolean.FALSE;&lt;br/&gt;
	private Long valueOne;&lt;/p&gt;

&lt;p&gt;	public Monitor() &lt;/p&gt;
{
		log.debug(&quot;CTOR&quot;);
	}

&lt;p&gt;	@Override&lt;br/&gt;
	public Boolean isEnabled() &lt;/p&gt;
{
		return enabled;
	}
&lt;p&gt;	@Override&lt;br/&gt;
	public void setEnabled(Boolean enabled) &lt;/p&gt;
{
		this.enabled = enabled;
	}

&lt;p&gt;	@Override&lt;br/&gt;
	public Long getValueOne() &lt;/p&gt;
{
		return valueOne;
	}
&lt;p&gt;	@Override&lt;br/&gt;
	public void setValueOne(Long valueOne) &lt;/p&gt;
{
		this.valueOne = valueOne;
	}

&lt;p&gt;}&lt;/p&gt;

&lt;p&gt;&amp;lt;/pre&amp;gt;&lt;/p&gt;

&lt;p&gt;And JMX MXBean&lt;/p&gt;

&lt;p&gt;&amp;lt;pre&amp;gt;&lt;br/&gt;
import javax.management.MXBean;&lt;/p&gt;

&lt;p&gt;@MXBean&lt;br/&gt;
public interface MonitorMXBean &lt;/p&gt;
{

	void setEnabled(Boolean enabled);
	Boolean isEnabled();

	void setValueOne(Long valueOne);
	Long getValueOne();

}

